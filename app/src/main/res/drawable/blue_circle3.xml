            <stroke android:width="0.11mm"/>
            <gradient android:angle="135"
                      android:startColor="@color/blueCircleLight"
                      android:endColor="@color/blueCircleDark"
                      android:centerX="100" />
        </shape>
    </item>

    <item android:left="8pt"
          android:top="3pt"
          android:width="15pt"
          android:height="15pt">
        <shape android:shape="oval">
            <stroke android:width="0.11mm"/>
            <gradient android:angle="135"
                      android:startColor="@color/blueCircleLight"
                      android:endColor="@color/blueCircleDark"
                      android:centerX="100" />
        </shape>
    </item>

    <item android:top="5pt"
          android:left="2pt"
          android:width="15pt"
          android:height="15pt">
        <shape android:shape="oval">

            <stroke android:width="0.11mm"/>
            <gradient android:angle="135"
                      android:startColor="@color/blueCircleLight"
                      android:endColor="@color/blueCircleDark"
                      android:centerX="100" />
        </shape>
    </item>



    <item android:height="25pt" android:width="25pt">
        <shape android:shape="rectangle">
        </shape>
    </item>




</layer-list>